<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	<bean id="helloWorld" class="com.bingley.spring.createobject.HelloWorld"></bean>
  	<!-- 
  		factory-method
  		  工厂方法
  	 -->
	<bean id="helloFactory" class="com.bingley.spring.createobject.HelloWorldFactory"></bean>

	<!--这个是怎么定义的-->
	<bean id="aa" factory-bean="helloFactory" factory-method="getIns"></bean>
</beans>
